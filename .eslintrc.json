{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": [
        "@babel/plugin-syntax-import-attributes"
      ]
    }
  },
  "extends": [
    "@adobe/eslint-config-aio-lib-config"
  ],
  "settings": {
      "jsdoc": {
        "ignorePrivate": true
      }
  },
  "rules": {
    "node/no-missing-import": 0,
    "jsdoc/require-jsdoc": 0
  }
}
